  },
  "devDependencies": {
    "@types/source-map-support": "^0.5.4",
    "react-router": "workspace:*",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react-router": "workspace:*",
    "typescript": "^5.1.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/",
    "globals.d.ts",
    "install.d.ts",
    "install.js",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ]
